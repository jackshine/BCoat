<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="DCI Test Suite" parallel="tests" thread-count="20">

	<listeners>
		<listener class-name="com.elastica.beatle.PriorityListener" />
	</listeners>

	<test name="DCI Foreign Language Chinese False Positive Tests" group-by-instances="true"
		preserve-order="true">
		<parameter name="environmentName" value="prod" />
		<parameter name="cloudSocUName" value="box-admin" />
		<parameter name="tenantName" value="dcibeatlecom" />
		<parameter name="domainName" value="dcibeatle.com" />
		<parameter name="saasApp" value="Box" />
		<parameter name="apiBaseVersion" value="v1" />
		<groups>
			<run>
				<include name="Chinese" />
			</run>
		</groups>
		<classes>
			<class name="com.elastica.beatle.tests.dci.risks.DCIForeignLanguageFalsePositiveTests"></class>
		</classes>
	</test>

