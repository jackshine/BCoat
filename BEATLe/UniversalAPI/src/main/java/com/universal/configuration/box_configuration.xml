<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment>Box Configuration</comment>
	
	<entry key="box.networkprotocol">https</entry>
	<entry key="box.host">app.box.com</entry>
	<entry key="box.port"></entry>
	<entry key="connectionHeader">keep-alive</entry>
	<entry key="box.admin.username">qa-admin@elasticaqa.net</entry>
	<entry key="box.admin.password">Cloudsoc@123</entry>
	<entry key="box.authorization.url">https://app.box.com/api/oauth2/authorize?response_type=code&amp;client_id={client_id}&amp;state=security_token%3DKnhMJatFipTAnM0nHlZA</entry>
	
	
	<entry key="box.uploadsfolder.path">//src/test//resources//uploads//box//</entry>
	<entry key="box.downloadsfolder.path">//src/test//resources//downloads//box//</entry>
	
	<entry key="box.uri.getDefaultDrive">/2.0/folders/0</entry>
	<entry key="box.uri.getUserInfo">/2.0/users/me</entry>
	<entry key="box.uri.getDownloadFile">/2.0/files/{file_id}/content</entry>
	<entry key="box.uri.getFileInfo">/2.0/files/{file_id}</entry>
	<entry key="box.uri.getFileFromTrash">/2.0/files/{file_id}/trash</entry>
	<entry key="box.uri.updateFile">/2.0/files/{file_id}/content</entry>
	<entry key="box.uri.copyFile">/2.0/files/{file_id}/copy</entry>
	
	
	<entry key="box.uri.createFolder">/2.0/folders</entry>
	<entry key="box.uri.getFolderInfo">/2.0/folders/{folder_id}</entry>
	<entry key="box.uri.updateFolder">/2.0/folders/{folder_id}</entry>
	<entry key="box.uri.deleteFolder">/2.0/folders/{folder_id}</entry>
	<entry key="box.uri.copyFolder">/2.0/folders/{folder_id}/copy</entry>
	<entry key="box.uri.getFolderItems">/2.0/folders/{folder_id}/items</entry>
	<entry key="box.uri.folderCollaborations">/2.0/folders/{folder_id}/collaborations</entry>
	<entry key="box.uri.getFolderTrashedItems">/2.0/folders/trash/items</entry>
	<entry key="box.uri.trashedFolder">/2.0/folders/{folder_id}/trash</entry>
	<entry key="box.uri.restoreFolder">/2.0/folders/{folder_id}</entry>
	
	
	
	
	

</properties>
 